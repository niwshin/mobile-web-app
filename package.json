{
  "name": "mobile-web-app",
  "version": "1.0.0",
  "description": "Web Application with Mobile UX",
  "main": "index.ts",
  "scripts": {
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "webpack:build": "webpack --config webpack.config.js",
    "webpack:watch": "webpack --watch --config webpack.config.js",
    "webpack:start": "webpack serve --config webpack.config.js",
    "build": "run-s tsc webpack:build",
    "watch": "run-p tsc:watch webpack:watch",
    "start": "run-p tsc:watch webpack:start",
    "lint": "eslint --cache src/",
    "lint:eslint": "eslint --cache --fix src/",
    "lint:prettier": "prettier --write .",
    "lint:fix": "run-s lint:eslint lint:prettier",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/niwshin/mobile-web-app.git"
  },
  "author": "niwshin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/niwshin/mobile-web-app/issues"
  },
  "homepage": "https://github.com/niwshin/mobile-web-app#readme",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.19.4",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.10",
    "html-webpack-plugin": "^5.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  }
}
